<div class="Grueso">
    <div class="form-row">
        <div class="form-group col-md-3">
            <label>CURP del aspirante:</label>
            <input type="text" readonly class="form-control-plaintext" name="CURP" disabled value="<%=JsSoliM.CURP%>">
        </div>
        <div class="form-group col-md-2">
            <label>Estatus de la solicitud:</label>
            <input type="text" id="status" readonly class="form-control-plaintext" name="STATUS" disabled value="<%=JsSoliM.ValidacionD%>">
        </div>
    </div>
    <div class="form-group">
        <label>Comentario Agregado:</label> 
        <textarea id="Atxt" readonly class="form-control-plaintext" name="Coment" rows="3" disabled value="<%=JsSoliM.Comentario%>"><%=JsSoliM.Comentario%></textarea>
    </div>
    <h4>I. Datos generales</h4>
            <div class="form-row">
                <div class="form-group col-md-5">
                <label>Nombre del solicitante:</label>
                <input type="text" readonly class="form-control-plaintext" name="NOM" disabled value="<%=JsSoliM.nombre%>">
                </div>
                <div class="form-group col-md-5">
                    <label>Lugar de nacimiento:</label>
                <input type="text" readonly class="form-control-plaintext" name="LUGAR" disabled value="<%=JsSoliM.lugar_nacimiento%>">
                </div>
                <div class="form-group col-md-2">
                    <label>Fecha de nacimiento:</label>
                    <input type="text" readonly class="form-control-plaintext" name="FECHA" disabled value="<%=JsSoliM.fecha_nacimiento%>">
                </div>
            </div>
            <div class="form-group">
                <label>Direccion del solicitante</label>
                <input type="text" readonly class="form-control-plaintext" name="DIRC" disabled value="<%=JsSoliM.direccion%>">
            </div>
            <div class="form-row">
                <div class="form-group col-md-4">
                    <label>Numero celular de contacto:</label>
                    <input type="text" readonly class="form-control-plaintext" name="CEL" disabled value="<%=JsSoliM.celular%>">
                </div>
                <div class="form-group col-md-4">
                    <label>Nacionalidad:</label>
                    <input type="text" readonly class="form-control-plaintext" name="NACI" disabled value="<%=JsSoliM.nacionalidad%>">
                </div>
                <div class="form-group col-md-4">
                    <label>Estado civil:</label>
                <input type="text" readonly class="form-control-plaintext" name="CIV" disabled value="<%=JsSoliM.estado_civil%>">
                </div>
            </div>
            <div class="form-row">
                <div class="form-group col-md-3">
                    <label>Cuenta de correo electronico:</label>
                    <input type="text" readonly class="form-control-plaintext" name="CORREO" disabled value="<%=JsSoliM.correo%>">
                </div>
                <div class="form-group col-md-3">
                    <label>Cuenta de SKYPE:</label>
                    <input type="text" readonly class="form-control-plaintext" name="SKYPE" disabled value="<%=JsSoliM.skype%>">
                </div>
            </div>
</div>