<div class="Grueso">
    <h4>II. Informacion general</h4>
    <div class="form-row">
        <div class="form-group col-md-8">
        <label>Institución donde realizo sus estudios el solicitante:</label>
        <input type="text" readonly class="form-control-plaintext" name="institucionD" disabled value="<%= JsSoliD.institucionD%>">
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-4">
            <!--Se cambia el texto dependiendo el formulario-->
            <label>Graduado:</label>
            <input type="text" readonly class="form-control-plaintext" name="graduadoD" disabled value="<%= JsSoliD.graduadoD%>">
        </div>
        <div class="form-group col-md-4">
            <label>Posgrado:</label>
            <input type="text" readonly class="form-control-plaintext" name="posgradoD" disabled value="<%= JsSoliD.posgradoD%>">
        </div>
        <div class="form-group col-md-4">
            <label>Pais de la institucion:</label>
            <input type="text" readonly class="form-control-plaintext" name="paisinstD" disabled value="<%= JsSoliD.paisinstD%>">
        </div>
    </div>
    <div class="form-group">
        <label>Experiencia Profesional:</label>
        <textarea id="Atxt" readonly class="form-control-plaintext" name="experienciaD" rows="3" disabled ><%= JsSoliD.experienciaD%></textarea>
    </div>
    <div class="form-group">
        <label>Experiencia en Docencia:</label>
        <textarea id="Atxt" readonly class="form-control-plaintext" name="experienciadD" rows="3" disabled ><%= JsSoliD.experienciadD%></textarea>
    </div>
    <!--Si tomo algun curso prosedeutico que se muestren los años-->

    <%if(JsSoliD.Anio4||JsSoliD.Anio3){%>
        <div class="form-group">
            <label>Años que asistió a los cursos propedéuticos:</label>
        </div>
    <div class="form-row">
        <div class="form-group col-md-3">
            <input type="text" readonly class="form-control-plaintext" name="Anio4" disabled value="<%= JsSoliD.Anio3%>">
        </div>
        <div class="form-group col-md-3">
            <input type="text" readonly class="form-control-plaintext" name="Anio3" disabled value="<%= JsSoliD.Anio4%>">
        </div>
    </div>
    <%}else{%>
        <div class="form-group">
            <label>Este aspirante no ha asistido a los cursos propedéuticos</label>
        </div>  
        <%}%>
    <!--Si tomo algun curso prosedeutico que se muestren los años-->
    <div class="form-group">
        <label>Motivo del ingreso:</label>
        <textarea id="Atxt" readonly class="form-control-plaintext" name="motivoD" rows="3" disabled ><%= JsSoliD.motivoD%></textarea>
    </div>

    <div class="form-group col-md-6">
        <label>La linea de investigacion deseada por el solicitante:</label>
        <%if(JsSoliD.lineaD=="DeM"){%>
        <input type="text" readonly class="form-control-plaintext" name="lineaD" disabled value="Docencia en matemáticas">
        <%}else if(JsSoliD.lineaD=="DdlF"){%>
        <input type="text" readonly class="form-control-plaintext" name="lineaD" disabled value="Didáctica de la Física">    
        <%}else if(JsSoliD.lineaD=="TICEF"){%>
        <input type="text" readonly class="form-control-plaintext" name="lineaD" disabled value="Tecnologías de la Información y la Comunicación en la Enseñanza de la Física">    
        <%}else if(JsSoliD.lineaD=="FD"){%>
            <input type="text" readonly class="form-control-plaintext" name="lineaD" disabled value="Formación Docente">
        <%}%>
    </div>

    <div class="form-row">
        <div class="form-group col-md-3">
            <label>Fecha de Envio:</label>
            <input type="text" readonly class="form-control-plaintext" name="firmaD" disabled value="<%=JsSoliD.fechahoyD%>">
        </div>
        <%if(JsSoliD.firmaD){%>
        <div class="form-group col-md-9">
            <label>Firma Electronica del aspirante:</label>
            <input type="text" readonly class="form-control-plaintext" name="FIRMAV" disabled value="<%= JsSoliD.firmaD%>">
        </div>
        <%}else{%>
            <div class="form-group">
                <label>Este aspirante no cuenta con firma electronica</label>
            </div>  
            <%}%>
    </div>  
</div>